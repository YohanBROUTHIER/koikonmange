{
  "name": "koikonmange",
  "version": "0.0.1",
  "description": "Vacilitateur de choix de plats.",
  "type": "module",
  "scripts": {
    "db:create": "psql -U koikonmange -d koikonmange -f ./data/create_tables.sql",
    "db:seed": "node ./data/recipes_seeding.js",
    "db:reset": "npm run db:create && npm run db:seed",
    "dev:back": "nodemon index.js",
    "dev:front": "vite ./app",
    "preview": "vite preview ./app",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "build": "vite build --emptyOutDir ./app",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "start": "node index.js"
  },
  "keywords": [
    "koikonmange"
  ],
  "author": "gyoza@oclock.io",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "sanitize-html": "^2.11.0",
    "url-query-json-parser": "^1.0.6"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.1.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.54.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.5",
    "multi-range-slider-react": "^2.0.7",
    "react-select": "^5.8.0",
    "supertest": "^6.3.4",
    "vite": "^5.0.8"
  },
  "main": "index.js"
}
